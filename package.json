{
  "name": "gateway",
  "private": true,
  "version": "1.2.1",
  "license": "MIT",
  "scripts": {
    "types": "tsc --noEmit",
    "circular": "madge --circular --extensions ts ./ --tsConfig tsconfig.json",
    "test": "jest",
    "dev": "NODE_ENV=development nodemon --config ./nodemon.json",
    "build": "tsc && cp -a ./templates ./dist",
    "pretty": "prettier --write .",
    "commit": "git-cz",
    "lint:code": "eslint \"./**/*.{ts,tsx}\"",
    "release": "standard-version"
  },
  "dependencies": {
    "@breadhead/detil-ts": "^1.0.1",
    "@checkmoney/soap-opera": "^1.9.0",
    "@nestjs/common": "^5.6.2",
    "@nestjs/core": "^5.6.2",
    "@nestjs/jwt": "^0.2.1",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "@solid-soda/cache": "^0.4.1",
    "@solid-soda/config": "^2.2.0",
    "@solid-soda/evolutions": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "fast-deep-equal": "^2.0.1",
    "google-auth-library": "^5.2.0",
    "handlebars": "^4.1.0",
    "http-proxy-middleware": "^1.0.5",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "nanoid": "^2.0.1",
    "nest-telegram": "^0.4.0",
    "pg": "^7.8.0",
    "promise-timeout": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "string-similarity": "^3.0.0",
    "tsconfig-paths": "^3.7.0",
    "tsoption": "^0.7.0",
    "typeorm": "^0.2.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/cors": "^2.8.4",
    "@types/jest": "^24.0.14",
    "@types/md5": "^2.1.33",
    "@types/string-similarity": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-unicorn": "^18.0.1",
    "fast-check": "^1.15.1",
    "husky": "^4.2.5",
    "jest": "^24.8.0",
    "jest-module-name-mapper": "^0.1.2",
    "lint-staged": "^10.1.3",
    "madge": "^3.4.3",
    "nodemon": "^1.18.9",
    "prettier": "^2.0.4",
    "rimraf": "^2.6.2",
    "standard-version": "^8.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx,scss,css,js,json,md}": [
      "prettier --write"
    ]
  }
}
