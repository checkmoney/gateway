{
  "name": "isapp",
  "private": true,
  "workspaces": [
    "back"
  ],
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "types": "tsc --noEmit",
    "circular": "madge --circular --extensions ts ./ --tsConfig tsconfig.json",
    "test": "jest",
    "s": "yarn soda",
    "dev": "NODE_ENV=development nodemon --config ./nodemon.json",
    "build": "tsc && cp -a ./templates ../dist/back",
    "start": "pm2 start ./pm2.config.js"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "tsconfig-paths": "^3.7.0",
    "tsoption": "^0.7.0",
    "@breadhead/detil-ts": "^1.0.1",
    "@checkmoney/soap-opera": "^1.9.0",
    "@nestjs/common": "^5.6.2",
    "@nestjs/core": "^5.6.2",
    "@nestjs/jwt": "^0.2.1",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "@solid-soda/cache": "^0.4.1",
    "@solid-soda/config": "^2.2.0",
    "@solid-soda/evolutions": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "fast-deep-equal": "^2.0.1",
    "google-auth-library": "^5.2.0",
    "handlebars": "^4.1.0",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "nanoid": "^2.0.1",
    "nest-telegram": "^0.4.0",
    "pg": "^7.8.0",
    "promise-timeout": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "string-similarity": "^3.0.0",
    "typeorm": "^0.2.12"
  },
  "devDependencies": {
    "@solid-soda/scripts": "^1.14.2",
    "@types/jest": "^24.0.14",
    "fast-check": "^1.15.1",
    "jest": "^24.8.0",
    "jest-module-name-mapper": "^0.1.2",
    "madge": "^3.4.3",
    "pm2": "^3.2.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "@types/cors": "^2.8.4",
    "@types/md5": "^2.1.33",
    "@types/string-similarity": "^3.0.0",
    "nodemon": "^1.18.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn soda lint-staged",
      "pre-push": "yarn soda lint && yarn types && yarn circular",
      "commit-msg": "yarn soda commitlint"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
